{
  "name": "twish",
  "version": "1.0.0",
  "author": "nasum",
  "scripts": {
    "build": "rm -rf build && webpack --color",
    "build:watch": "rm -rf build && webpack --watch --color",
    "build:production:mac": "rm -rf build && webpack -p && electron-packager ./build production --out=pack --platform=darwin --arch=x64 --electron-version=1.6.2 --overwrite",
    "electron": "yarn run build && electron ./build/browser/app.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.2.0",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "react": "^15.4.2"
  }
}
